{
	"name": "copycat",
	"displayName": "CopyCat",
	"description": "Easily clone extensions from inside VSCode!",
	"version": "0.0.1",
	"icon": "images/icon.png",
	"license": "LICENSE",
	"publisher": "jmg-duarte",
	"author": {
		"name": "Jos√© Duarte"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:copycat.copyExtension"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "copycat.copyExtension",
				"title": "üêà CopyCat: Copy Extension"
			}
		]
	},
	"repository": {
		"url": "https://github.com/jmg-duarte/copycat",
		"type": "git"
	},
	"bugs": {
		"url": "https://github.com/jmg-duarte/copycat/issues"
	},
	"homepage": "https://github.com/jmg-duarte/copycat",
	"engines": {
		"vscode": "^1.63.0"
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.63.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"ts-loader": "^9.2.5",
		"webpack": "^5.52.1",
		"webpack-cli": "^4.8.0",
		"@vscode/test-electron": "^1.6.2"
	}
}